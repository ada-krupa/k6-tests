{
    "name": "k6-tests",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build --path tsconfig.build.json",
        "start": "nest start --path tsconfig.json",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "format": "yarn prettier -w ./src ./test-k6-examples",
        "lint": "eslint \"{src,apps,libs,test,test-k6-examples}/**/*.ts\"",
        "docker-build": "docker build --build-arg NODE_VERSION=$(cat .nvmrc | sed 's/^v//') -t k6-tests .",
        "docker-run": "docker run --rm -p 3000:3000 k6-tests"
    },
    "dependencies": {
        "@nestjs/common": "11.1.0",
        "@nestjs/config": "4.0.2",
        "@nestjs/core": "11.1.0",
        "@nestjs/platform-express": "11.1.0",
        "@nestjs/swagger": "11.2.0",
        "@nestjs/terminus": "11.0.0",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.2",
        "date-fns": "4.1.0",
        "express-basic-auth": "1.2.1",
        "helmet": "8.1.0",
        "ramda": "0.30.1",
        "reflect-metadata": "0.2.2",
        "rxjs": "7.8.2"
    },
    "devDependencies": {
        "@nestjs/cli": "11.0.7",
        "@nestjs/schematics": "11.0.5",
        "@nestjs/testing": "11.1.0",
        "@types/express": "5.0.1",
        "@types/k6": "0.57.1",
        "@types/node": "22.15.17",
        "@types/ramda": "0.30.2",
        "eslint": "9.26.0",
        "eslint-config-codemask": "2.1.3",
        "prettier": "3.5.3",
        "rimraf": "^6.0.1",
        "source-map-support": "0.5.21",
        "ts-loader": "9.5.2",
        "ts-node": "10.9.2",
        "tsconfig-paths": "4.2.0",
        "typescript": "5.8.3"
    }
}
